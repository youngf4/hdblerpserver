<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hdbl.erp.dao.TestUserDao">

    <select id="getUserById" resultType="com.hdbl.erp.entity.UserBean">
        select * from tb_user where id = #{id}
    </select>

    <select id="getAllUsers" resultType="com.hdbl.erp.entity.UserBean">
        select * from tb_user
    </select>

</mapper>