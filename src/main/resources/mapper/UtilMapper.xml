<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hdbl.erp.dao.UtilDao">
    <!--<select id="getMaxValue">-->
    <!--</select>-->
    <select id="getMaxValue" resultType="HashMap">
                select value from tb_max_value where name=#{name};
                update tb_max_value set value = value+1
    </select>
</mapper>